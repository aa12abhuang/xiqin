<view><scroll-view class="{{['page',modalName!=null?'show':'']}}" scroll-y="{{modalName==null}}"><view class="cu-bar bg-white solid-bottom margin-top"><view class="action"><text class="cuIcon-title text-orange"></text>{{''+fixtoday+' 整改安排'}}</view><view class="action"></view></view><view class="{{['cu-list','menu',menuBorder?'sm-border':'',menuCard?'card-menu margin-top':'']}}"><block wx:for="{{fixlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{item.date==fixtoday}}"><view class="{{['cu-item',menuArrow?'arrow':'']}}"><view class="content"><text class="cuIcon-btn text-green"></text><text class="text-grey display-font " style="margin-left:30rpx;">{{"期限: "+item.ddl}}</text><text class="text-grey block-display display-font" style="margin-left:30rpx;">{{"发布人: "+item.people}}</text><text class="text-grey block-display display-font" style="margin-left:30rpx;">{{"地点: "+item.locate}}</text><text class="text-grey block-display display-font" style="margin-left:30rpx;">{{"内容: "+item.content}}</text><text class="text-grey block-display display-font" style="margin-left:30rpx;">{{"备注要求: "+item.content}}</text></view><view class="action"><button data-event-opts="{{[['tap',[['clickupload']]]]}}" class="cu-btn round bg-green shadow" bindtap="__e"><text class="cuIcon-upload"></text>上传</button></view></view></block></block></view></scroll-view></view>